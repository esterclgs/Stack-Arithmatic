#include <iostream>
#include <cstring>
using namespace std;
Template <typename T>
class stack{
	T *data;
	int size;
	int top;
public:
	stack(){
	size=0;
	data=new T[size];
	top=0;}
	
	stack(int s){
	size=s;
	data=new T[size];
	top=0;}

	bool isFull(){
	return (top==size);}
	
	bool isEmpty(){
	return (top==0);}
	
